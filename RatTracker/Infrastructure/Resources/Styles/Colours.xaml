<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <SolidColorBrush x:Key="BrushEliteOrange" Color="#FFFF7100" />
  <SolidColorBrush x:Key="BrushEliteGreen" Color="#FF029E4C" />
  <SolidColorBrush x:Key="BrushEliteBlue" Color="#FF00B3F7" />
  <SolidColorBrush x:Key="BrushTransparent" Color="#00000000" />
  <SolidColorBrush x:Key="BrushBlack" Color="#FF000000" />

  <SolidColorBrush x:Key="RequestStateAccepted" Color="LightGreen" />
  <SolidColorBrush x:Key="RequestStatePending" Color="Orange" />
  <SolidColorBrush x:Key="RequestStateNotReceived" Color="Red" />

</ResourceDictionary>